    "%sql\n",
    "\n",
    "SELECT USAGE_DATE, SUM(USAGE_QUANTITY) AS DBUs\n",
    "FROM system.billing.usage\n",
    "where year(usage_date) = year(CURRENT_DATE)\n",
    "group by usage_date\n",
    "order by usage_date asc"
   ]
  }
